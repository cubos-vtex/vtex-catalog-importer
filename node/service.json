{
  "memory": 1024,
  "ttl": 1440,
  "timeout": 60,
  "cpu": {
    "type": "shared",
    "value": 5,
    "scalingPercentage": 20
  },
  "minReplicas": 8,
  "maxReplicas": 80,
  "workers": 1,
  "events": {
    "onAppInstalled": {
      "sender": "apps",
      "keys": ["setup"],
      "settingsType": "workspace"
    },
    "onSettingsChanged": {
      "sender": "apps",
      "keys": ["settings"]
    }
  }
}
